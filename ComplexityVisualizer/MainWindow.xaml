<Window x:Class="ComplexityVisualizer.MainWindow"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="479" Width="800">
    <Grid Margin="0,0,0,17">
        <lvc:CartesianChart x:Name="enumChart" Height="154" VerticalAlignment="Top" />
        <lvc:CartesianChart x:Name="listChart" Margin="0,142,0,0" Height="154" VerticalAlignment="Top" />
        <lvc:CartesianChart x:Name="hashSetChart" Margin="0,294,0,0" Height="154" VerticalAlignment="Top" />
        <Label Content="Enum" Background="White" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top"/>
        <Label Content="List" Background="White" HorizontalAlignment="Left" Margin="170,148,0,0" VerticalAlignment="Top"/>
        <Label Content="HashSet" Background="White" HorizontalAlignment="Left" Margin="170,290,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
